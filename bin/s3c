#!/bin/bash


    # script
    script_path=$( dirname $0 )

    # aws - s3 - cp
    aws s3 cp "$script_path/../index.html" 's3://s3.magento2setup.com/index.html'

    # aws - s3 - cp - ubuntu-14.04
    aws s3 cp "$script_path/../ubuntu-14.04-lts/bin/getup" 's3://s3.magento2setup.com/ubuntu-14.04-lts/bin/getup'
    aws s3 cp "$script_path/../ubuntu-14.04-lts/bin/setup" 's3://s3.magento2setup.com/ubuntu-14.04-lts/bin/setup'


